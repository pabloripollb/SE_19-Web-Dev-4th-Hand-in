    <script>
        // --- 1. Lógica para el Menú Hamburguesa (Copiada de tu index.html) ---
        const hamburgerBtn = document.getElementById('hamburger-btn');
        const mainNav = document.getElementById('main-nav');
        
        if (hamburgerBtn && mainNav) {
            hamburgerBtn.addEventListener('click', () => {
              mainNav.classList.toggle('nav-open');
              hamburgerBtn.classList.toggle('is-active');
              const isExpanded = mainNav.classList.contains('nav-open');
              hamburgerBtn.setAttribute('aria-expanded', isExpanded);
            });
        }

        // --- 2. NUEVA Lógica para el Toggle de Precios ---
        
        document.addEventListener('DOMContentLoaded', () => {

            const billingToggle = document.getElementById('billing-switch');
            
            const priceElements = document.querySelectorAll('.plan-price');

            if (billingToggle && priceElements.length > 0) {
                
                billingToggle.addEventListener('change', () => {
                    
                    const isYearly = billingToggle.checked;

                    priceElements.forEach(priceEl => {
                        
                        if (isYearly) {
                            priceEl.textContent = priceEl.dataset.yearly;
                        } else {
                            priceEl.textContent = priceEl.dataset.monthly;
                        }
                    });
                });
            }
        });
    

</script>
